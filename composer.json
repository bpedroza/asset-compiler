{
    "name": "bpedroza/asset-compiler",
    "description": "An asset compiler for php. Compile multiple javascript or css files into a single file.",
    "keywords": ["asset", "loader", "compiler", "laravel", "minify", "minification"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Pedroza"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.6.0",
        "matthiasmullie/minify": "1.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Bpedroza\\AssetCompiler\\": "src/AssetCompiler"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
